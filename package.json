{
  "name": "rapidroute",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "format": "pnpm run lint:css && eslint . --fix",
    "lint:css": "npx stylelint './app/**/*.{js,jsx,ts,tsx}' --fix || exit 0",
    "start": "next start",
    "typegen": "npx supabase gen types typescript --linked --schema public > app/types/supabase.ts",
    "test": "vitest",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@linaria/babel-preset": "5.0.4",
    "@linaria/react": "5.0.3",
    "@material-symbols/svg-700": "0.14.0",
    "@prisma/client": "5.5.2",
    "@svgr/webpack": "8.1.0",
    "@t3-oss/env-nextjs": "0.7.1",
    "@tanstack/react-query": "5.17.19",
    "@types/flexsearch": "0.7.6",
    "ahooks": "3.7.8",
    "color2k": "2.0.2",
    "comlink": "4.4.1",
    "flexsearch": "0.7.21",
    "google-spreadsheet": "4.1.1",
    "gsap": "3.12.2",
    "next": "14.0.2",
    "next-with-linaria": "0.6.0",
    "postcss": "8.4.31",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "the-new-css-reset": "1.11.1",
    "webpack": "5.89.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.5.1",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.2",
    "eslint-config-reform-collective": "github:reformcollective/eslint-config-reform",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "prisma": "5.5.2",
    "stylelint": "15.11.0",
    "stylelint-config-reform-collective": "github:reformcollective/stylelint-config-reform",
    "typescript": "5.2.2",
    "vitest": "0.34.6"
  },
  "ct3aMetadata": {
    "initVersion": "7.13.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@next/next/recommended",
      "reform-collective"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-reform-collective"
  },
  "browserslist": [
    "last 99 chrome versions and last 1 year",
    "last 99 ChromeAndroid versions and last 1 year",
    "last 99 Safari versions and last 1 year",
    "last 99 iOS versions and last 1 year",
    "last 99 firefox versions and last 1 year",
    "last 99 FirefoxAndroid versions and last 1 year"
  ],
  "pnpm": {
    "patchedDependencies": {
      "flexsearch@0.7.21": "patches/flexsearch@0.7.21.patch"
    }
  }
}
