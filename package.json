{
  "name": "@rapidroute/root",
  "author": "ScaryCrumb",
  "version": "1.0.0",
  "workspaces": [
    "packages/*",
    "apps/*",
    "services/*"
  ],
  "dependencies": {
    "prettier": "^2.7.1"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" && npm run lint -workspaces",
    "start": "npm run build -w ./packages/database-types/ && npm start -w ./packages/rapidroute/",
    "import": "npm run start -w ./packages/database-importer",
    "typegen": "npm run build -w ./packages/database-types/"
  }
}
