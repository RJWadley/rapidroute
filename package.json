{
  "name": "rapidroute",
  "version": "0.1.0",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "format": "pnpm run lint:css && eslint . --fix",
    "lint:css": "npx stylelint './app/**/*.{js,jsx,ts,tsx}' --fix || exit 0",
    "start": "next start",
    "typegen": "npx supabase gen types typescript --linked --schema public > app/types/supabase.ts",
    "test": "vitest",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@koale/useworker": "4.0.2",
    "@linaria/babel-preset": "5.0.4",
    "@linaria/react": "6.1.0",
    "@material-symbols/svg-700": "0.16.0",
    "@pixi/react": "7.1.1",
    "@pixi/react-animated": "7.1.1",
    "@prisma/client": "5.10.2",
    "@svgr/webpack": "8.1.0",
    "@t3-oss/env-nextjs": "0.9.2",
    "@tanstack/react-query": "5.24.6",
    "@types/flexsearch": "0.7.6",
    "ahooks": "3.7.10",
    "color2k": "2.0.3",
    "comlink": "4.4.1",
    "emulate-tab": "1.2.1",
    "flexsearch": "0.7.21",
    "google-spreadsheet": "4.1.1",
    "gsap": "3.12.5",
    "minisearch": "6.3.0",
    "next": "14.1.1",
    "next-with-linaria": "0.7.0",
    "penner": "0.1.3",
    "pixi-viewport": "5.0.2",
    "pixi.js": "7.4.0",
    "postcss": "8.4.35",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-spring": "9.7.3",
    "react-use-measure": "2.1.1",
    "the-new-css-reset": "1.11.2",
    "use-deep-compare": "1.2.1",
    "webpack": "5.90.3",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.5.1",
    "@types/node": "20.11.24",
    "@types/react": "18.2.61",
    "@types/react-dom": "18.2.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.1",
    "eslint-config-reform-collective": "github:reformcollective/eslint-config-reform",
    "eslint-plugin-chai-expect": "3.0.0",
    "eslint-plugin-chai-friendly": "0.7.4",
    "prisma": "5.10.2",
    "stylelint": "16.2.1",
    "stylelint-config-reform-collective": "github:reformcollective/stylelint-config-reform",
    "typescript": "5.3.3",
    "vitest": "1.3.1"
  },
  "ct3aMetadata": {
    "initVersion": "7.13.1"
  },
  "eslintConfig": {
    "extends": [
      "plugin:@next/next/recommended",
      "reform-collective"
    ]
  },
  "stylelint": {
    "extends": "stylelint-config-reform-collective"
  },
  "browserslist": [
    "last 99 chrome versions and last 1 year",
    "last 99 ChromeAndroid versions and last 1 year",
    "last 99 Safari versions and last 1 year",
    "last 99 iOS versions and last 1 year",
    "last 99 firefox versions and last 1 year",
    "last 99 FirefoxAndroid versions and last 1 year"
  ],
  "pnpm": {
    "patchedDependencies": {
      "flexsearch@0.7.21": "patches/flexsearch@0.7.21.patch"
    }
  }
}
